{{#if route.linkable}}
  {{#link-to route.path class=linkClass}}
    {{#if hasBlock}}
      {{yield this route}}
    {{else if i18n}}
      {{t route.i18nTitle}}  
    {{else}}
      {{route.title}}
    {{/if}}
  {{/link-to}}
{{else}}
  {{#if hasBlock}}
    {{yield this route}}
  {{else if i18n}}
    {{t route.i18nTitle}}  
  {{else}}
    {{route.title}}
  {{/if}}
{{/if}}
