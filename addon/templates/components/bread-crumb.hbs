{{#if route.linkable}}
  {{#link-to route.path class=linkClass}}
    {{#if hasBlock}}
      {{yield this route}}
    {{else}}
      {{route.title}}
    {{/if}}
  {{/link-to}}
{{else}}
  <span>
    {{#if hasBlock}}
      {{yield this route}}
    {{else}}
      {{route.title}}
    {{/if}}
  </span>
{{/if}}
